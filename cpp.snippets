# Get Primes till N
snippet getprimes
	vector<int> pr, lp;
	void initprimes(int pn) {
		lp.resize(pn + 1, 0);
		for (int i = 2; i <= pn; ++i) {
			if (lp[i] == 0) {
				lp[i] = i;
				pr.push_back(i);
			}
			for (int j = 0; j < (int)pr.size() && pr[j] <= lp[i] && i * pr[j] <= pn; ++j)
				lp[i * pr[j]] = pr[j];
		}
	}

