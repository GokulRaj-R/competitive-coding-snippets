# Binary Exponentiation
snippet pwr
	long long pwr(long long a, long long b, long long m = M) {
		long long ret = 1;

		while (b > 0) {
			if (b & 1)	ret = ret * a % m;
			a = a * a % m;
			b >>= 1;
		}
		return ret;
	}

# Prefix Function
snippet prefixfunction
	vector<int> prefix_function(string s) {
			int n = (int)s.size();
			vector<int> pi(n);
			for (int i = 1; i < n; i++) {
					int j = pi[i-1];
					while (j > 0 && s[i] != s[j])
							j = pi[j-1];
					if (s[i] == s[j])
							j++;
					pi[i] = j;
			}
			return pi;
	}

