# Polynomial Multiplication using FFT
snippet pmultiply
	template <typename T>
	vector<T> multiply(vector<T> const& a, vector<T> const& b) { /* {{{ */
		vector<T> fa(a.begin(), a.end()), fb(b.begin(), b.end());
		int n = 1;
		while (n < (int)a.size() + (int)b.size()) n <<= 1;
		
		if ((int)min(a.size(), b.size()) < 150) {
			vector<T> ret(n, 0);
			for (int i = 0; i < (int)a.size(); ++i) {
				for (int j = 0; j < (int)b.size(); ++j) {
					ret[i+j] += a[i] * b[j];
				}
			}
			return ret;
		}

		fa.resize(n, 0);
		fb.resize(n, 0);
		ntt(fa, 0);
		ntt(fb, 0);
		for (int i = 0; i < n; ++i) fa[i] *= fb[i];
		ntt(fa, 1);

		return fa;
	} /* }}} */
# Topological Sort
snippet topsort
	bool tcycle = 0;
	vector<int> tvis;

	void tdfs(int x, vector<int>& ord) { /* {{{ */
		if (tcycle) return ;
		tvis[x] = 2;
		for (auto v : g[x]) {
			if (tvis[v] == 2) tcycle = 1;
			else if (!tvis[v]) tdfs(v, ord);
		}
		tvis[x] = 1;
		ord.push_back(x);
	}
		
	vector<int> tsort() {
		vector<int> ord;
		tvis.resize(g.size());
		for (int i = 0; i < (int)g.size(); ++i) {
			if (!tvis[i] && !tcycle) {
				tdfs(i, ord);
			}
		}

		if (!tcycle) return ord;
		return vector<int> ();
	} /* }}} */
# Tree diameter
snippet dia
	void getdepth(int x, int &r, vector<int>& ddep, vector<int>& dpar) {
		ddep[x] = (x == r ? 0 : ddep[dpar[x]] + 1);
		if (ddep[x] > ddep[r]) r = x;
		for (auto v : g[x]) {
			if (v == dpar[x]) continue;
			dpar[v] = x;
			getdepth(v, r, ddep, dpar);
		}
	}

	vector<int> diameter(int s = 0) {
		int v = s;
		vector<int> ddep(g.size()), dpar(g.size());
		dpar[v] = v; getdepth(v, v, ddep, dpar);
		dpar[v] = v; getdepth(v, v, ddep, dpar);
		
		vector<int> ret(1, v);
		for (int i = v; i != dpar[i]; i = dpar[i]) 
			ret.push_back(dpar[i]);
		return ret;
	}
# Euler Totient Function
snippet phifunc
	vector<int> phi_1_to_n(int n) {
		vector<int> phi(n + 1);
		phi[0] = 0;
		phi[1] = 1;
		for (int i = 2; i <= n; i++)
			phi[i] = i;

		for (int i = 2; i <= n; i++) {
			if (phi[i] == i) {
				for (int j = i; j <= n; j += i)
					phi[j] -= phi[j] / i;
			}
		}

		return phi;
	}

