# Segment Tree
snippet segtree
	template <typename T>
	struct Segtree { /* {{{ */
		int N;
		vector<T> seg, lazy;
		Segtree(int _n) : N(_n) {
			seg.resize(4 * N, 0);
			lazy.resize(4 * N, 0);
		}

		T join(T x, T y) {
			return x + y;
		}

		void push(int x, int s, int e) {
			if (!lazy[x])	return;
			seg[x] += lazy[x];

			if (s != e) {
				lazy[2 * x] += lazy[x];
				lazy[2 * x + 1] += lazy[x];
			}
			lazy[x] = 0;
		}

		void build(int x, int s, int e) {
			if (s == e) {
				seg[x] = 0;
				return; 
			}

			int mid = (s + e) >> 1;
			build(2 * x, s, mid);
			build(2 * x + 1, mid + 1, e);

			seg[x] = join(seg[2 * x], seg[2* x + 1]);
		}

		void update(int x, int s, int e, int l, int r, T val) {
			push(x, s, e);
			if (s > r || e < l)	return ;

			if (s >= l && e <= r) {
				lazy[x] += val;
				push(x, s, e);
				return;
			}

			int mid = (s + e) >> 1;
			update(2 * x, s, mid, l, r, val);
			update(2 * x + 1, mid + 1, e, l, r, val);

			seg[x] = join(seg[2 * x], seg[2 * x + 1]);
		}

		T query(int x, int s, int e, int l, int r) {
			push(x, s, e);
			if (e < l || s > r)	return 0;
			if (s >= l && e <= r){
				return seg[x];
			}

			int mid = (s + e) >> 1;
			return join(query(2 * x, s, mid, l, r), query(2 * x + 1, mid + 1, e, l, r));
		}

		void update(int l, int r, T val) { update(1, 0, N - 1, l, r, val); }
		T query(int l, int r) { return query(1, 0, N - 1, l, r); }
	}; /* }}} */

