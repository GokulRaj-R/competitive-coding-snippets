# Floor Sum (A * i + B) / M
snippet floorsum
	long long floor_sum(long long n, long long m, long long a, long long b) {
		long long ret = 0;
		if (a >= m) {
			ret += n * (n - 1) * (a / m) / 2;
			a %= m;
		}

		if (b >= m) {
			ret += n * (b / m);
			b %= m;
		}
		
		long long last = a * n + b;
		if (last >= m) ret += floor_sum(last / m, a, m, last % m);
		return ret;
	}
# Extended Euclidean Algortihm
snippet exgcd
	ll exgcd(ll a, ll b, ll& x, ll& y) {
		if (!b) {
			x = 1; y = 0;
			return a;
		}

		ll x1, y1;
		ll g = exgcd(b, a % b, x1, y1);
		x = y1; y = x1 - y1 * (a / b);
		return g;
	}

# Chinese Reminder Theorem
snippet crt
	pair<long long, long long> crt(vector<long long> r, vector<long long> m) { /* {{{ */
		long long r0 = r[0], m0 = m[0];
		for (int i = 1; i < (int)r.size(); ++i) {
			if (m[i] > m0) {
				swap(m[i], m0);
				swap(r[i], r0);
			}

			if (m0 % m[i] == 0) {
				if (r0 % m[i] == r[i]) continue;
				return {-1, 0};
			}

			long long x, y, g;
			g = exgcd(m0, m[i], x, y);
			if ((r[i] - r0) % g) return {-1, 0};

			long long mg = m[i] / g;
			r0 += (r[i] - r0) / g % mg * x % mg * m0;
			m0 *= mg;

			if (r0 < 0) r0 += m0;
		}
		return {r0, m0}; 
	} /* }}} */

