	int n = s.size(), l = ceil(log2(n));
	lcp.resize(n, vector<int> (l + 1, 0));
	for (int i = 0, k = 0; i < n - 1; ++i) {
		int j = p[c[i] - 1];
		while (s[i + k] == s[j + k]) ++k;
		lcp[c[i] - 1][0] = k;
		k = max(k - 1, 0);
	}

	for (int i = 1; i <= l; ++i) {
		for (int j = 1; j + (1<<(i-1)) < n - 1; ++j) {
			lcp[j][i] = min(lcp[j][i - 1], lcp[j + (1<<(i-1))][i - 1]);
		}
	}

	auto lc = [&](int i, int j) {
		int len = log2(j - i + 1);
		return min(lcp[i][len], lcp[j - (1<<len) + 1][len]);
	};
